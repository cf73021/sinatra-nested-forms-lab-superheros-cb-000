
 <div class="team">
  <h1><%= @team.name %></h1><br>
  <h2>Team Motto: <%= @pirate.weight %> pounds</h2>
</div><br>

 <h1>Ships</h1>
 <% @ships.each do |ship| %>
   <div class="ship">

    <p>Name: <%= ship.name %></p><br>
    <p>Type: <%= ship.type %></p><br>
    <p>Booty: <%= ship.booty %></p>
  </div><br>
<% end %>
